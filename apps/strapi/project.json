{
  "name": "strapi",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/strapi",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi build",
        "cwd": "apps/strapi"
      }
    },
    "develop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi develop",
        "cwd": "apps/strapi"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi start",
        "cwd": "apps/strapi"
      }
    }
  },
  "tags": []
}
